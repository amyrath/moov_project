{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [],
    "watchAssets": true
  },
  "projects": {
    "app": {
      "type": "application",
      "root": "src",
      "entryFile": "main"
    },
    "extra": {
      "type": "library",
      "root": "src2"
    }
  }
}